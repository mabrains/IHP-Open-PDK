#-----------------------------------------------------------------------
# Special paint/erase rules
#-----------------------------------------------------------------------

compose
  compose  nfet  poly  ndiff
  compose  pfet  poly  pdiff
  compose  var   poly  nsd
  compose  pvar  poly  psd

  compose  hvnfet  poly  hvndiff
  compose  hvpfet  poly  hvpdiff

  decompose hvnfetesd poly hvndiff
  decompose hvpfetesd poly hvpdiff

  # Prevent m5 from erasing mimcap
  paint	 mimcap	 m5	mimcap
  paint	 mimcc	 m5	mimcc

  paint  ndc     nwell  pdc
  paint  nfet    nwell  pfet
  paint  ndiff   nwell  pdiff
  paint  psd     nwell  nsd
  paint  psc     nwell  nsc
  paint  ptap    nwell  ntap
  paint  ptapc   nwell  ntapc

  paint  pdc     pwell  ndc
  paint  pfet    pwell  nfet
  paint  pdiff   pwell  ndiff
  paint  nsd     pwell  psd
  paint  nsc     pwell  psc
  paint  ntap    pwell  ptap
  paint  ntapc   pwell  ptapc

  paint  m1      obsm1  m1
  paint  m2      obsm2  m2
  paint  m3      obsm3  m3
  paint  m4      obsm4  m4
  paint  m5      obsm5  m5
  paint  m6      obsm5  m6
  paint  m7      obsm5  m7

  # Seal ring contacts are not really contacts in
  # the database, so use compose rules instead.
  paint  sealc	 psd	sealc
  paint  sealv1	 m1	sealv1
  paint  sealv2	 m2	sealv2
  paint  sealv3	 m3	sealv3
  paint  sealv4	 m4	sealv4
  paint  sealv5	 m5	sealv5
  paint  sealv6	 m6	sealv6

  # To preserve keep-out areas for pattern fill,
  # do not allow comment to overwrite fillblock or inductor.
  paint  fillblock comment fillblock
  paint  inductor comment inductor

  # nwell itself is a p-implant blocker, so don't erase
  # nwell when painting pblock, even though overlap is
  # technically allowed according to the documentation.
  paint  nwell  pblock  nwell

end

#-----------------------------------------------------------------------
# Electrical connectivity
#-----------------------------------------------------------------------

connect
  *nwell,*nsd,*ntap,*hvnsd,*hvntap,dnwell,pnp *nwell,*nsd,*ntap,*hvnsd,*hvntap,dnwell,pnp
  pwell,*psd,*ptap,*hvpsd,*hvptap,digisub pwell,*psd,*ptap,*hvpsd,*hvptap,digisub
  # Note:  This connects butting taps even without a metal strap.
  *nsd,*hvnsd	*pdiff,*hvpdiff
  *psd,*hvpsd	*ndiff,*hvndiff
  *ntap,*hvntap	*pdiff,*hvpdiff
  *ptap,*hvptap	*ndiff,*hvndiff
  npn,pbc	npn,pbc
  pbc,*m1	pbc,*m1
  *m1,m1fill,iprobe,diffprobe	*m1,m1fill,iprobe,diffprobe
  *m2,m2fill	*m2,m2fill
  *m3,m3fill	*m3,m3fill
  *m4,m4fill	*m4,m4fill
  *m5,m5fill	*m5,m5fill
  *m6,m6fill	*m6,m6fill
  *m7,m7fill    *m7,m7fill
  *mimcap       *mimcap
   allnactivenonfet	allnactivenonfet
   # Resistor to isolated pwell tap
   isodiffres	*ndiff,*psd
   # Note:  This creates a short through hvndiffres resistors.
   *pdiff,*psd,*pdiode,*hvpdiff,*hvpsd *pdiff,*psd,*pdiode,*hvpdiff,*hvpsd
  *poly,allfets,polyfill	*poly,allfets,polyfill
end

